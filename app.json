{
  "expo": {
    "name": "Nitro",
    "slug": "Nitro",
    "scheme": "nitro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mart1n991.nitro",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.mart1n991.nitro",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "extra": {
      "CLERK_PUBLISHABLE_KEY": "__CLERK_PUBLISHABLE_KEY__",
      "DATABASE_URL": "__DATABASE_URL__",
      "router": {},
      "eas": {
        "projectId": "486b13e8-855c-4437-b1cc-1327bbeca8e9"
      }
    },
    "owner": "mart1n991"
  }
}
